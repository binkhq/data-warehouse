clean:
	dbt clean

compile: clean
	dbt compile --exclude +elementary

deps:
	dbt deps

run: compile
	dbt run --exclude +elementary

full_run: compile deps
	dbt run

test: compile deps
	dbt test

source_test: compile deps
	dbt test --select tag:source

output_tests: compile deps
	dbt test --exclude tag:source
	
docs:
	dbt docs generate
	dbt docs serve